<p id="notice"><%= notice %></p>

<h2>Showing Call List</h2>

<fieldset>
  <legend><strong>CSR Detail</strong></legend>
  <p>
    <strong>Day:</strong>
    <%= @calllist.calllists_day %>&emsp;&emsp;
    <strong>CSR:</strong>
    <%= @calllist.csr %>&emsp;&emsp;
    <strong>Rep:</strong>
    <%= @calllist.rep %>&emsp;&emsp;
    <strong>Called?:</strong>
    <%= human_boolean(@calllist.called) %>&emsp;&emsp;
    <strong>Ordered?:</strong>
    <%= human_boolean(@calllist.ordered) %>
  </p>
</fieldset>
<p>
</p>

<fieldset>
  <legend><strong>Customer Detail</strong></legend>
  <p>
    <strong>Customer Code:</strong>
    <%= @calllist.custcode %>&emsp;&emsp;
    <strong>Ship To:</strong>
    <%= @calllist.shipto %>&emsp;&emsp;
    <strong>Dept:</strong>
    <%= @calllist.dept %>
    <!-- <strong>Item:</strong> -->
    <!-- <%= @calllist.item %> -->
  </p>
  <p>
    <strong>Phone:</strong>
    <%= @calllist.phone %>&emsp;&emsp;
    <strong>Manager:</strong>
    <%= @calllist.manager %>&emsp;&emsp;
    <strong>Notes:</strong>
    <%= @calllist.notes %>
    <!-- <strong>Total Items:</strong> -->
    <!-- <%= @calllist.totalitems %> -->
  </p>
</fieldset>
<p>
</p>

