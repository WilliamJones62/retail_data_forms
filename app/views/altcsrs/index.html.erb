<p id="notice"><%= notice %></p>

<h1>Alternate CSRs</h1>

<table class="mytable" class="display" cellspacing="0" >
  <thead>
    <tr>
      <th>Usual CSR</th>
      <th>Alternate CSR</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @altcsrs.each do |altcsr| %>
      <tr>
        <td><%= altcsr.usualcsr %></td>
        <td><%= altcsr.altcsr %></td>
        <td><%= altcsr.start %></td>
        <td><%= altcsr.end %></td>
        <td><%= link_to 'Show', altcsr %></td>
        <td><%= link_to 'Edit', edit_altcsr_path(altcsr) %></td>
        <td><%= link_to 'Delete', altcsr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alt CSR', new_altcsr_path %>
